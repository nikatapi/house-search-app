<li class="list-group-item", id="<%= feed_item.id %>">
	<% link_to gravatar_for(feed_item.user), feed_item.user %>
	<span class="user">
		<%= link_to feed_item.user.name, feed_item.user %>
	</span>
	
	<span class="country"> Country: <%= feed_item.country %></span>,
	<span class="city">City:<%= feed_item.city %></span>,
	<span class="address">Address:<%= feed_item.address %></span>
	<span class="description">Description:<%= feed_item.description %></span>
	<span class="price">Price:<%= feed_item.price %>â‚¬</span>
	

	
	<%= link_to "delete", feed_item, method: :delete, data: { confirm: "You sure?" }, title: feed_item.description %>
</li>